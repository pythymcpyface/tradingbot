{
  "name": "tradingbot-glicko",
  "version": "1.0.0",
  "description": "High-performance cryptocurrency trading bot using Glicko-2 ratings",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/node-api/index.ts",
    "build": "npm run build:rust && npm run build:ts",
    "build:rust": "cd src/rust-core && cargo build --release",
    "build:ts": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "setup": "./scripts/setup-database.sh",
    "db:migrate": "npx ts-node scripts/migrate-database.ts",
    "db:calculate-ratings": "npx ts-node scripts/calculate-initial-ratings.ts",
    "db:reset": "npx prisma migrate reset && npm run db:migrate && npm run db:calculate-ratings",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "getTradingPairs": "npx ts-node scripts/getTradingPairs.ts",
    "getKlines": "npx ts-node scripts/getKlines.ts",
    "getKlines:1m": "npx ts-node scripts/getKlines.ts",
    "getKlines:5m": "npx ts-node scripts/getKlines.ts",
    "getKlines:1h": "npx ts-node scripts/getKlines.ts",
    "getKlines:1d": "npx ts-node scripts/getKlines.ts",
    "calculateGlickoRatings": "npx ts-node scripts/calculateGlickoRatings-fixed.ts",
    "calculateGlickoRatings:chunked": "npx ts-node scripts/calculateGlickoRatings-chunked.ts",
    "calculateGlickoRatings:broken": "npx ts-node scripts/calculateGlickoRatings.ts",
    "compareGlickoAlgorithms": "npx ts-node scripts/compare-glicko-algorithms.ts",
    "plotGlickoRatings": "npx ts-node scripts/plotGlickoRatings.ts",
    "startTrading": "npx ts-node scripts/startTradingEngine.ts",
    "startPaperTrading": "npx ts-node scripts/startTradingEngine.ts --paper",
    "runWindowedBacktest": "npx ts-node scripts/runWindowedBacktest.ts",
    "runAllWindowedBacktests": "npx ts-node scripts/runAllWindowedBacktests.ts",
    "runAllWindowedBacktestsForPair": "npx ts-node scripts/runAllWindowedBacktestsForPair.ts --",
    "runAllWindowedBacktestsForPair-parallel": "npx ts-node scripts/runAllWindowedBacktestsForPair-parallel.ts",
    "generateOptimizationReport": "npx ts-node scripts/generateOptimizationReport.ts",
    "optimize-database-performance": "npx ts-node scripts/optimize-database-performance.ts",
    "preload-backtest-data": "npx ts-node scripts/preload-backtest-data.ts",
    "runWindowedBacktest-optimized": "npx ts-node scripts/runWindowedBacktest-optimized.ts",
    "runAllWindowedBacktests-parallel": "npx ts-node scripts/runAllWindowedBacktests-parallel.ts",
    "validate-performance-improvements": "npx ts-node scripts/validate-performance-improvements.ts",
    "validate-performance-simple": "npx ts-node scripts/validate-performance-simple.ts",
    "analyzeBacktestSuccess": "npx ts-node scripts/analyzeBacktestSuccess.ts",
    "generate3DOptimizationReport": "npx ts-node scripts/generate3DOptimizationReport.ts",
    "compareOptimizationApproaches": "npx ts-node scripts/compareOptimizationApproaches.ts",
    "runWindowedBacktest-enhanced": "npx ts-node scripts/runWindowedBacktest-enhanced.ts",
    "compareWalkForwardMethods": "npx ts-node scripts/compareWalkForwardMethods.ts",
    "calculateCalmarRatios": "npx ts-node scripts/calculateCalmarRatios.ts",
    "generateCalmarRatioReport": "npx ts-node scripts/generateCalmarRatioReport.ts",
    "queryTopCalmarRatios": "npx ts-node scripts/queryTopCalmarRatios.ts",
    "queryTopReturns": "npx ts-node scripts/queryTopReturns.ts",
    "queryTopDrawdowns": "npx ts-node scripts/queryTopDrawdowns.ts",
    "queryTopSharpe": "npx ts-node scripts/queryTopSharpe.ts",
    "queryTopAlpha": "npx ts-node scripts/queryTopAlpha.ts",
    "generateTradeCharts": "npx ts-node scripts/generateTradeCharts.ts",
    "compareTopPerformers": "npx ts-node scripts/compareTopPerformers.ts",
    "findOptimalParameters": "npx ts-node scripts/findOptimalParameters.ts",
    "clear:all": "npx ts-node scripts/clear-database.ts",
    "clear:optimization": "npx ts-node scripts/clear-optimization-results.ts",
    "clear:orders": "npx ts-node scripts/clear-backtest-orders.ts",
    "clear:runs": "npx ts-node scripts/clear-backtest-runs.ts",
    "clear:production": "npx ts-node scripts/clear-production-orders.ts",
    "clear:ratings": "npx ts-node scripts/clear-glicko-ratings.ts",
    "clear:klines": "npx ts-node scripts/clear-klines.ts",
    "validate:klines": "npx ts-node scripts/validate-klines-data.ts",
    "getKlines:turbo": "npx ts-node scripts/getKlines-turbo.ts",
    "getKlines:bulk": "npx ts-node scripts/getKlines-bulk.ts",
    "getKlines:resume": "npx ts-node scripts/getKlines-resume.ts"
  },
  "keywords": [
    "trading",
    "cryptocurrency",
    "glicko-2",
    "backtesting",
    "binance"
  ],
  "author": "Trading Bot System",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/axios": "^0.14.4",
    "@types/ws": "^8.5.10",
    "axios": "^1.11.0",
    "binance-api-node": "^0.12.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "lru-cache": "^10.1.0",
    "node-cache": "^5.1.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/lru-cache": "^7.10.10",
    "@types/node": "^20.10.4",
    "@types/node-cache": "^4.2.5",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "pg": "^8.16.3",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
